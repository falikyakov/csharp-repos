
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>

    @{

        for (int i = 0; i < 10; i++)
        {
            <tr>
                for (int j = 1; j < 10; j++)
                {
                <td>i*j</td>
                }
            </tr>
        }
    }
    <br />
    <br />

    <br />

    <script>
        <table><tr>
        for (var i = 1; i < 11; i++) {
          <td>  for (var j = 1; j < 11; j++) {
                document.write(i * j);
           </td> }
        }
         </tr>   </table>
    </script>

</body>

</html>
